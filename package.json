{
  "name": "start-backbone-webpack",
  "version": "1.0.0",
  "description": "my start-backbone project using webpack as opposed to browserify",
  "scripts": {
    "server": "npm run lint && webpack-dev-server --content-base ./static --progress --colors",
    "bundle": "webpack --progress --colors",
    "watch": "webpack --watch --progress --colors",
    "lint": "jscs --config ./.jscsrc ./app/ && jshint --config .jshintrc ./app/"
  },
  "keywords": [
    "webpack",
    "backbone"
  ],
  "author": "Austin Burnett",
  "license": "ISC",
  "devDependencies": {
    "backbone": "^1.1.2",
    "jquery": "^2.1.3",
    "jscs": "^1.10.0",
    "jshint": "^2.5.11",
    "jshint-stylish": "^1.0.0",
    "webpack": "^1.5.1",
    "webpack-dev-server": "^1.7.0"
  }
}
